 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Service Order Form</title>\n    <link rel=\"stylesheet\" href=\"styles.css\">\n   <head>\n    <style>\n        /* Import Source Sans Pro from Google Fonts */\n\n/* Apply Source Sans Pro to the body and other elements */\nbody {\n    font-family: 'Source Sans Pro', sans-serif;\n}\n.col-xs-4.column-break {\n    width: 13.3%;\n}\n@media screen {\n  .table {\n       margin-right: 60px;\n    }\n\n}\n\n@page {\n    margin-top: 70px;\n    margin-bottom: 70px;\n}\n.form-headers {\n        position: flex;\n        margin-top: -66px;\n    }\n    .form-header div {\n   \n    margin-right: 1px;\n}\n\n/* Apply styles for printing */\n@media print {\n    body {\n        margin: 0;\n        padding: 0;\n        font-family: 'Source Sans Pro', sans-serif;\n    }\n    \n    .headers-html {\n        width: 30%; /* Set the desired width for the letterhead */\n    }\n    .form-headers {\n     \n        margin-top: -25px;\n    }\n    .table {\n        margin-left: 370px;\n    }\n\n    .service-order-form {\n        padding-bottom: -20px;\n        margin-left: 0px;\n    }\n\n    .additional-info {\n        padding: 2px;\n    }\n\n    .payment-terms {\n        padding-top: 20px;\n        margin-top: -22px;\n    }\n}\n\n/* Table borders */\n.item-table {\n    width: 100%;\n    border-collapse: collapse; /* Ensure that borders do not double up */\n    font-family: 'Source Sans Pro', sans-serif;\n}\n\n.item-table th, .item-table td {\n    border: 2px solid #e0dcdc; /* Add border for all cells */\n    padding: 8px; /* Add some padding for better spacing */\n}\n\n.item-table th:last-child, .item-table td:last-child {\n    border-right: 2px solid #e0dcdc; /* Ensure right border is visible */\n}\n.print-format .row:not(.section-break) {\n    line-height: 1.6;\n    margin-top: 1px !important;\n}\n\n\n    </style>\n<body style=\"font-family:Source Sans Pro\">\n    \n<div>\n    <div class=\"service-order-form\">\n        <div style=\"width:70%;\"><strong style=\"font-size: 22pt;margin-left: -15px;\">PROFORMA INVOICE</strong></p></div>\n        <p style=\"font-size:12pt;margin-left: -14px;\">Invoice No: <strong style=\"font-size:12pt; font-weight:bold; \"> {{doc.name}} </strong></p>\n        </div>\n\n        <div id=\"headers-html\" style=\"width:48%; float:right; margin-top: -114px; margin-left: -18px;\">\n<img src=\"/assets/service_pro/img/abc.jpeg\" alt=\"Letterhead\" style=\"width: 100%;\" /></div>\n\n\n<div class=\"row\" style=\"text-transform: uppercase;margin-right: -165px;\">\n    <!-- Left Column for Customer and Executive Details -->\n    <div class=\"col-xs-6\" style=\"margin-left: 9px;\">>\n        {% if doc.customer_name %}\n            <div class=\"row\">\n                <div class=\"col-xs-4\" style=\"font-size: 11pt;\">Customer Name:</div>\n                <div class=\"col-xs-8\" style=\" margin-left: -9px;font-size: 11pt;\"><b>{{ doc.customer_name }}</b></div>\n            </div>\n        {% endif %}\n\n        {% if doc.address_display %}\n            <div class=\"row\">\n                <div class=\"col-xs-4\" style=\"font-size: 11pt;\">Customer Address:</div>\n                <div class=\"col-xs-8\" style=\"margin-left: -8px;font-size: 11pt;\">{{ doc.address_display }}</div>\n            </div>\n        {% endif %}\n    </div>\n\n    <!-- Right Column for Date, RFQ No, and Valid Till -->\n    <div class=\"col-xs-6\" style=\"margin-right: -10px;\">\n        {% if doc.transaction_date %}\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\" style=\"font-size: 11pt;\">Date:</div>\n                <div class=\"col-xs-8\" style=\"font-size: 11pt;\">{{ doc.get_formatted(\"transaction_date\") }}</div>\n            </div>\n        {% endif %}\n\n\n        {% if doc.delivery_date %}\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\" style=\"font-size: 11pt;\">Delivery Date:</div>\n                <div class=\"col-xs-8\" style=\"font-size: 11pt;\">{{ doc.get_formatted(\"delivery_date\") }}</div>\n            </div>\n        {% endif %}\n    </div>\n</div>\n\n    <div>\n    {% if doc.items and doc.items|length > 0 %}\n    <table class=\"item-table\" >\n        <thead>\n            <tr>\n                <th>Sr</th>\n                <th>Item Description</th>\n                <th>Qty</th>\n                <th>UOM</th>\n                <th>Rate</th>\n                <th>Amount</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% for row in doc.items %}\n            <tr>\n                <td>{{ loop.index }}</td> <!-- Serial number -->\n                <td style=\"width: 60%;word-break: break-all;\">{{ row.description }}</td>\n                <td>{{ row.qty }}</td>\n                <td>{{ row.uom }}</td>\n                <td>{{ row.rate }}</td>\n                <td>{{ row.amount }}</td>\n            </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n    {% else %}\n    <p>.</p>\n    {% endif %}\n</div>\n\n        </div>\n  <div class=\"total\" style=\"padding-bottom: 20px; display: flex; justify-content: flex-end;margin-right: -175px;\">\n    <table class=\"table\" style=\"width:50%; margin-top:1px;\">\n        <tbody>\n            <tr style=\"border:hidden;\">\n                <td style=\"font-size: 12pt; border:hidden; line-height:9px;\">Total</td>\n                <td style=\"font-size: 12pt; border:hidden; line-height:0;\">\n                    {{ doc.get_formatted(\"total\") }}\n                </td>\n            </tr>\n\n            <!-- Show Net Total only if Discount exists -->\n            {% if doc.additional_discount_amount != 0 %}\n            <tr>\n                <td style=\"font-size: 12pt; border:hidden; line-height:9px;\">Net Total</td>\n                <td style=\"font-size: 12pt; border:hidden;  line-height:0;\">\n                    {{ doc.get_formatted(\"net_total\") }}\n                </td>\n            </tr>\n            {% endif %}\n\n            {% if doc.tax_amount != 0 %}\n            {% if doc.total_taxes_and_charges %}\n            <tr style=\"border:hidden;\">\n                <td style=\"font-size: 12pt; border:hidden; line-height:9px;\">Tax Amount</td>\n                <td style=\"font-size: 12pt; border:hidden;  line-height:0;\">\n                    {{ doc.get_formatted(\"total_taxes_and_charges\") }}\n                </td>\n            </tr>\n            {% endif %}\n            {% endif %}\n\n            <!-- Conditional check for discount -->\n            {% if doc.additional_discount_amount != 0 %}\n            {% if doc.discount_amount %}\n            <tr>\n                <td style=\"font-size: 12pt; border:hidden; line-height:9px;\">Discount</td>\n                <td style=\"font-size: 12pt; border:hidden;  line-height:0;\">\n                    {{ doc.get_formatted(\"discount_amount\") }}\n                </td>\n            </tr>\n            {% endif %}\n            {% endif %}\n            \n            <tr style=\"border:hidden;\">\n                <td style=\"font-size: 12pt; font-weight: bold; border:hidden; line-height:9px;\">Grand Total</td>\n                <td style=\"font-weight: bold; font-size: 12pt; border:hidden;  line-height:0;\">\n                    {{ doc.get_formatted(\"grand_total\") }}\n                </td>\n            </tr>\n        </tbody>\n    </table>\n</div>\n\n\n\n        <div class=\"additional-info\">\n            <p style=\"font-size: 12pt;margin-top: -38px;margin-left: 3px;\">Total Qty: {{doc.total_qty}}</p>\n            <p style=\"font-size: 12pt;\"><strong style=\"font-size: 12pt;margin-left: 3px;\">In Words:</strong> {{doc.in_words}} </p>\n            {% if doc.custom_company_bank_details %}\n            <td style=\"border-bottom: hidden;border-right: hidden;border-left: hidden;border-top: hidden;font-size: 12pt;\"><strong style=\"font-size: 12pt;\"> Company Bank Details: </strong></p></td></tr>\n            {% if frappe.db.get_value(\"Company Bank Detailes\", doc.custom_company_bank_details, \"bank_account_no\") %}\n               <tr><p style=\"text-align:left;font-size: 11pt;\"><td style=\"border-bottom: hidden;border-right: hidden;border-left: hidden\"> Ac No :{{frappe.db.get_value(\"Company Bank Detailes\",doc.custom_company_bank_details,\"bank_account_no\")}}</p></td></tr>\n               {% endif %}\n            {% if frappe.db.get_value(\"Company Bank Detailes\", doc.custom_company_bank_details, \"iban\") %}\n               <tr><p style=\"text-align:left;font-size: 11pt;\"><td style=\"border-bottom: hidden;border-right: hidden;border-left: hidden\"> IBN No:{{frappe.db.get_value(\"Company Bank Detailes\",doc.custom_company_bank_details,\"iban\")}}</p></td></tr>\n               {% endif %}\n            {% if frappe.db.get_value(\"Company Bank Detailes\", doc.custom_company_bank_details, \"bank\") %}\n               <tr><p style=\"text-align:left;font-size: 11pt;\"><td style=\"border-bottom: hidden;border-right: hidden;border-left: hidden\"> Bank Name:{{frappe.db.get_value(\"Company Bank Detailes\",doc.custom_company_bank_details,\"bank\")}}</p></td></tr>\n               {% endif %}\n                {% endif %}\n        </div>\n        {% if doc.payment_terms %}\n        <div class=\"payment-terms\">\n            <p style=\"font-size: 12pt;font-family:Source Sans Pro;margin-left: 3px;\"><strong style=\"font-size: 12pt; padding-top:25;\">Payment Terms: </strong>{{doc.payment_terms}}</p>\n\n        </div>\n        {% endif %}\n        {% if doc.terms %}\n        <div class=\"terms\">\n            <p style=\"font-size: 12pt;margin-left: 3px;\"><strong>Terms:</strong></p>\n            <p style=\"font-size: 12pt;margin-left: 3px;\">{{doc.terms}} </p>\n        </div>\n        {% endif %}\n    </div>\n\n</body>\n</html>",
