 "doctype": "Print Format",
 "font_size": 14,
 "html": "\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css\" rel=\"stylesheet\" integrity=\"sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH\" crossorigin=\"anonymous\">\n    <title>Service Order Form</title>\n    <link rel=\"stylesheet\" href=\"styles.css\">\n   <head>\n    <style>\n        /* Import Source Sans Pro from Google Fonts */\n        @import url('https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@300;400;600;700&display=swap');\n\n        /* Apply Source Sans Pro to the body and other elements */\n        body {\n            font-family: 'Source Sans Pro';\n        }\n\n        @page {\n            margin-top: 70px;\n            margin-bottom: 70px;\n        }\n\n        /* Apply styles for printing */\n        @media print {\n            body {\n                margin: 0;\n                padding: 0;\n                font-family: 'Source Sans Pro', sans-serif;\n            }\n            .table {\n                margin-left: 350px;\n            }\n\n            .ms-3 {\n                margin-top: 20px;\n                padding-top: -5px;\n                padding: 2px;\n                margin-left:500px;\n            }\n\n            .me-3 {\n                margin-top: 17px;\n                padding: 2px;\n            }\n\n            .service-order-form {\n                padding-bottom: -20px;\n                margin-left:0px;\n            }\n\n            .additional-info {\n                padding: 2px;\n            }\n\n            .payment-terms {\n                padding-top: 20px;\n            }\n        }\n\n        /* Table borders */\n        .item-table {\n            width: 100%;\n            border-collapse: collapse; /* Ensure that borders do not double up */\n            font-family: 'Source Sans Pro';\n        }\n\n        .item-table th, .item-table td {\n            border: 2px solid #e0dcdc; /* Add border for all cells */\n            padding: 8px; /* Add some padding for better spacing */\n            font-family: 'Source Sans Pro', sans-serif;\n        }\n\n        /* Specific border adjustments */\n        .item-table th:last-child, .item-table td:last-child {\n            border-right: 2px solid #e0dcdc; /* Ensure right border is visible */\n        }\n    </style>\n</head>\n\n</head>\n<body style=\"font-family:Source Sans Pro\">\n    \n<div >\n<div>\n    <div class=\"service-order-form\">\n        <div style=\"width:70%;\"><strong style=\"font-size: 27pt;margin-left: -15px;\">Service Order Form</strong></p></div>\n        <p style=\"font-size:12pt;margin-left: -14px;\">Reference No: <strong style=\"font-size:12pt; font-weight:bold; \"> {{doc.name}} </strong></p>\n        </div>\n        <header>\n        <div id=\u201cheader-html\u201d style=\"width:30%; float:right; margin-top: -120px; margin-left: -40px;\">{{ letter_head or ' '}} </div>\n    </header>\n        <div class=\"form-header\">\n            <div class=\"ms-3\">  \n                <p style=\"font-size:12pt; padding-bottom: 2; margin-top:-43px;margin-left:-11px;\">To: <strong style=\"font-size:12pt; font-weight:bold;\">{{doc.customer_name}}</strong></p>\n                \n                <p style=\"font-size:12pt; padding-bottom: 7;margin-left:-11px;\">Sales Executive:<strong style=\"font-size:12pt; font-weight:bold;\">{{doc.sales_executive}}</strong></p>\n                <p style=\"font-size:12pt; padding-bottom: 7;margin-left:-11px;\">Operation Executive:<strong>{{doc.operation_executive}}</strong></p>\n            </div>\n            <div class=\"me-3\" style=\"float: right;\">\n                <p style=\"font-size:12pt; padding-bottom: 2px;margin-top:-43px;margin-left: 100px;\">Date:<strong style=\"font-size:12pt; font-weight:bold;\">{{doc.get_formatted(\"posting_date\")}}</strong></p>\n                <p style=\"font-size:12pt; padding-bottom: 2px;margin-left: 100px;\">RFQ No:<strong style=\"font-size:12pt; font-weight:bold;\">{{doc.rfq_no}}</strong></p>\n                <p style=\"font-size:12pt; padding-bottom: 2px;margin-left: 100px;\">Valid Till:<strong style=\"font-size:12pt; font-weight:bold;\">{{doc.get_formatted(\"valid_till\")}}</strong></p>\n                \n            </div>\n    </div>\n\n    <div><table class=\"item-table\">\n            <thead>\n               \n                <tr> \n                    <th>Description</th>\n                    <th>Qty</th>\n                    <th>UOM</th>\n                    <th>Rate</th>\n                    <th>Discount</th>\n                    <th>Amount</th>\n                </tr>\n            </thead>\n             {% for row in doc.option1 %}\n            <tbody>\n                <tr>\n                    <td class=\"description\">{{row.item_description}}</td>\n                    <td>{{row.qty}}</td>\n                    <td>{{row.uom}}</td>\n                    <td>{{row.rate}}</td>\n                    <td>{{row.discount}}</td>\n                    <td>{{row.amount}}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n            \n               \n        \n        </table>\n        </div>\n  <div style=\"padding-bottom: 20px; display: flex; justify-content: flex-end;\">\n    <table class=\"table\" style=\"width:50%; margin-top:1px;\">\n        <tbody>\n            <tr style=\"border:hidden;\">\n                <td style=\"font-size: 12pt; border:hidden; line-height:0;\">Total</td>\n                <td style=\"font-weight: bold; font-size: 12pt; border:hidden; text-align: right; line-height:0;\">\n                    {{ doc.get_formatted(\"total\") }}\n                </td>\n            </tr>\n\n            <!-- Show Net Total only if Discount exists -->\n            {% if doc.additional_discount_amount != 0 %}\n            <tr>\n                <td style=\"font-size: 12pt; border:hidden; line-height:0;\">Net Total</td>\n                <td style=\"font-weight: bold; font-size: 12pt; border:hidden; text-align: right; line-height:0;\">\n                    {{ doc.get_formatted(\"net_total\") }}\n                </td>\n            </tr>\n            {% endif %}\n\n            {% if doc.tax_amount != 0 %}\n            <tr style=\"border:hidden;\">\n                <td style=\"font-size: 12pt; border:hidden; line-height:0;\">Tax Amount</td>\n                <td style=\"font-weight: bold; font-size: 12pt; border:hidden; text-align: right; line-height:0;\">\n                    {{ doc.get_formatted(\"tax_amount\") }}\n                </td>\n            </tr>\n            {% endif %}\n\n            <!-- Conditional check for discount -->\n            {% if doc.additional_discount_amount != 0 %}\n            <tr>\n                <td style=\"font-size: 12pt; border:hidden; line-height:0;\">Discount</td>\n                <td style=\"font-weight: bold; font-size: 12pt; border:hidden; text-align: right; line-height:0;\">\n                    {{ doc.get_formatted(\"additional_discount_amount\") }}\n                </td>\n            </tr>\n            {% endif %}\n            \n            <tr style=\"border:hidden;\">\n                <td style=\"font-size: 12pt; font-weight: bold; border:hidden; line-height:0;\">Grand Total</td>\n                <td style=\"font-weight: bold; font-size: 12pt; border:hidden; text-align: right; line-height:0;\">\n                    {{ doc.get_formatted(\"grand_total\") }}\n                </td>\n            </tr>\n        </tbody>\n    </table>\n</div>\n\n\n\n        <div class=\"additional-info\">\n            <p style=\"font-size: 12pt;margin-top: -38px;margin-left: 3px;\">Total Qty: {{doc.total_quantity}}</p>\n            <p style=\"font-size: 12pt;\"><strong style=\"font-size: 12pt;margin-left: 3px;\">Delivery Period:</strong> {{doc.delivery_period}}</p>\n            <p style=\"font-size: 12pt;\"><strong style=\"font-size: 12pt;margin-left: 3px;\">In Words:</strong> {{doc.in_words}} </p>\n        </div>\n        {% if doc.payment_terms %}\n        <div class=\"payment-terms\">\n            <p style=\"font-size: 12pt;padding-top: 15;font-family:Source Sans Pro;margin-left: 3px;\"><strong style=\"font-size: 12pt; padding-top:25;\">Payment Terms:</strong></p>\n            <p style=\"font-size: 12pt;font-family:Source Sans Pro;margin-left: 3px;\">{{doc.payment_terms}}</p>\n        </div>\n        {% endif %}\n        {% if doc.term_details %}\n        <div class=\"terms\">\n            <p style=\"font-size: 12pt;font-family:Source Sans Pro;margin-left: 3px;\"><strong>Terms:</strong></p>\n            <p style=\"font-size: 12pt;font-family:Source Sans Pro;margin-left: 3px;\">{{doc.term_details}} </p>\n        </div>\n        {% endif %}\n    </div>\n</div>\n</body>\n</html>\n",
